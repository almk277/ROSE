.module answer

.sub pi
.var a
.var b
.var pi
	flt.new a 6.28
	flt.new b 2.0
	flt.div pi a b
	dbg.flt pi
	dbg.byte '\n'
	return

.sub quest
.var a
.var b
.var ans
	int.byte a 1
	int.inc a 1
	int.new b 40
	int.add ans a b
	dbg.int ans
	dbg.byte '\n'
	return

.sub main
.export main
	call quest
	call pi
	exit
